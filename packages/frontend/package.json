{
  "name": "@goal-mandala/frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "vitest --run src/test/performance.test.tsx",
    "test:accessibility": "tsx src/test/accessibility-test-runner.ts",
    "test:accessibility:unit": "vitest --run src/__tests__/accessibility-*.test.{ts,tsx}",
    "test:accessibility:e2e": "playwright test src/test/e2e/accessibility.e2e.test.ts",
    "test:accessibility:watch": "vitest src/__tests__/accessibility-*.test.{ts,tsx}",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 30",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@goal-mandala/shared": "workspace:*",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.3.2",
    "aws-amplify": "^6.0.7",
    "dompurify": "^3.2.7",
    "react": "^18.2.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@storybook/addon-docs": "^9.1.9",
    "@storybook/addon-onboarding": "^9.1.9",
    "@storybook/react-vite": "^9.1.9",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^9.1.9",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^9.1.9",
    "tailwindcss": "^3.3.6",
    "terser": "^5.44.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "hono@>=4.8.0 <4.9.6": ">=4.9.6",
      "vite@<=5.4.19": ">=5.4.20",
      "hono@<4.9.7": ">=4.9.7"
    }
  }
}
