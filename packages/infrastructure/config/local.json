{
  "stackPrefix": "goal-mandala-local",
  "region": "ap-northeast-1",
  "database": {
    "instanceClass": "serverless",
    "minCapacity": 0.5,
    "maxCapacity": 1,
    "backupRetentionDays": 1,
    "deletionProtection": false
  },
  "lambda": {
    "timeout": 30,
    "memorySize": 256
  },
  "frontend": {
    "domainName": null,
    "certificateArn": null,
    "customErrorResponses": true
  },
  "monitoring": {
    "logRetentionDays": 7,
    "enableDetailedMonitoring": false
  },
  "tags": {
    "Environment": "local",
    "CostCenter": "development"
  }
}
